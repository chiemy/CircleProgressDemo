<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgress">
        <attr name="textColor" format="color" />
        <attr name="centreSectionColor" format="color"></attr>
        <attr name="progressColor" format="color"></attr>
        <attr name="textSize" format="dimension" />
		<attr name="progressWidth" format="dimension"></attr>        
		<attr name="radius" format="dimension"></attr>
		<attr name="maxProgress" format="integer"></attr>
		<attr name="progressBgColor" format="color"></attr>
		<attr name="style" >
		    <enum name="stroke" value="1"></enum>
		    <enum name="fill" value="2"></enum>
		</attr>
    </declare-styleable>
</resources>
